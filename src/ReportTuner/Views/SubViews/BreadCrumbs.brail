<% if IsDefined("breadcrumbs"): %>
    <div id="breadcrumb" style="font-family: Verdana;">
      <a href="${siteroot}/Reports/base.aspx">Настройка отчетов</a>
      <% for label, url in breadcrumbs: %>
      <span>&gt;</span>
      <% if url != "": %>
      <a href="${url}">${label}</a>
      <% else: %>
      <span style="font-weight:bold">${pageTitle}</span>
      <% end %>
      <% end %>
    </div>
<% end %>